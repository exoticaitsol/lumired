{% schema %}
  {
    "name": "Customer Say",
    "settings": [
      {
        "type": "text",
        "id": "main",
        "label": " Heading"
      },
      {
        "type": "image_picker",
        "id": "slide-prev",
        "label":" Slide prev"
      },
      {
        "type": "image_picker",
        "id": "slide-next",
        "label":" Slide Next"
      }
    ],
     "blocks": [
    {
      "type": "slider_testimonial_sec",
      "name": "slider_testimonial_sec",
      "settings": [
       
        {
        "type": "image_picker",
        "id": "img",
        "label":" Image"
      },
         {
          "type": "richtext",
          "id": "description",
          "label": "Description"
          
        },
         {
        "type": "text",
        "id": "heading",
        "label": " Heading"
      }
       
      ]
    }
  ],
    "presets": [
       {
       "name": "Customer Say"
       }
     ]
  }
{% endschema %}

