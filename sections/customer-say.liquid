<section class="testimonial_slider py_9">
        <div class="container">
          <div class="main_custom-txt">
            <div class="tittle_heading_say">
              <h2>{{ section.settings.main}}</h2>
            </div>
            <ul class="flex_arrow">
              <li class="slide_controls_1 text-left">
                <div
                  class="slide-prev slick-arrow slick-disabled"
                  aria-disabled="true"
                  style=""
                >
                  <img src="{{ section.settings.slide-prev | img_url: 'master'}}" />
                </div>
                <div
                  class="slide-next slick-arrow"
                  aria-disabled="false"
                  style=""
                >
                  <img src="{{ section.settings.slide-next | img_url: 'master'}}" />
                </div>
              </li>
            </ul>
          </div>

    
          
          <div class="slider_testimonial_sec">
            {% for block in section.blocks %}
            <div class="slider_under_bx">
              <div class="slider_under">
                <div class="slider_txt">
                  <div class="img_slide">
                    <img src="{{ block.settings.img | img_url: 'master'}}" alt="" />
                    {{ block.settings.description }}

                    <h2>{{ block.settings.heading }}</h2>
                  </div>
                </div>
              </div>
            </div>
