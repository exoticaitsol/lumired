<script>
  document.addEventListener("DOMContentLoaded", function() {
    var relieveTitles = document.querySelectorAll(".relieve_pain_txt p");
    relieveTitles.forEach(function(title) {
      if (title.textContent.trim() === "Free Irish Shipping") {
        title.textContent = "Free UK Shipping";
      }
    });
  });
</script>
{% endif %}

<script>
$(document).ready(function() {
    $('#yourTextAreaId').keydown(function(event) {
        if (event.keyCode === 13) {  // 13 is the key code for Enter key
            var content = $(this).html();
            $(this).html(content + '<br>');
            event.preventDefault();
        }
    });
});
</script>
